[9.363857, "o", "# 1. Update package list\r\nsudo apt update\r\n\r\n# 2. Install Python 3 + pip + some useful tools\r\nsudo apt install -y python3 python3-pip python3-venv git curl\r\n\r\n# 3. Make sure pip3 is available (creates a symlink if needed)\r\nsudo update-alternatives --install /usr/bin/pip pip /usr/bin/pip3 1\r\n\r\n# 4. Now install asciinema for your user only (recommended in WSL)\r\npip3 install --user asciinema\r\n\r\n# 5. Ensure ~/.local/bin is in your PATH (add it permanently)\r\necho 'export PATH=\"$HOME/.local/bin:$PATH\"' >> ~/.bashrc\r\nsource ~/.bashrc\r\n"]
